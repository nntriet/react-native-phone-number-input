{
    "name": "@linhnguyen96114/react-native-phone-input",
    "version": "3.3.2",
    "description": "React Native component for phone number",
    "source": "./src/index.tsx",
    "main": "./lib/commonjs/index.js",
    "module": "./lib/module/index.js",
    "typings": "./index.d.ts",
    "types": "./index.d.ts",
    "exports": {
        ".": {
            "types": "./index.d.ts",
            "import": "./lib/module/index.js",
            "require": "./lib/commonjs/index.js",
            "default": "./lib/module/index.js"
        }
    },
    "files": [
        "src",
        "lib",
        "android",
        "ios",
        "cpp",
        "*.podspec",
        "!.tmp",
        "!lib/typescript/example",
        "!ios/build",
        "!android/build",
        "!android/gradle",
        "!android/gradlew",
        "!android/gradlew.bat",
        "!android/local.properties",
        "!**/__tests__",
        "!**/__fixtures__",
        "!**/__mocks__",
        "!**/.*",
        "index.d.ts"
    ],
    "scripts": {
        "example": "yarn workspace react-native-phone-number-input-example",
        "test": "jest",
        "typecheck": "tsc",
        "lint": "eslint \"**/*.{js,ts,tsx}\"",
        "lint:fix": "eslint \"**/*.{js,ts,tsx}\" --fix",
        "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build lib",
        "prepare": "bob build",
        "release": "release-it"
    },
    "keywords": [
        "react-native",
        "ios",
        "android"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/linhnguyen-gt/react-native-phone-number-input.git"
    },
    "author": "Linh Nguyen <linhnguyen96114@gmail.com> (https://github.com/linhnguyen-gt/react-native-phone-number-input)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/linhnguyen-gt/react-native-phone-number-input/issues"
    },
    "homepage": "https://github.com/linhnguyen-gt/react-native-phone-number-input#readme",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "devDependencies": {
        "@commitlint/config-conventional": "^17.0.2",
        "@evilmartians/lefthook": "^1.5.0",
        "@react-native-community/cli": "15.0.0",
        "@react-native/eslint-config": "^0.73.1",
        "@release-it/conventional-changelog": "^5.0.0",
        "@types/google-libphonenumber": "^7.4.30",
        "@types/jest": "^29.5.5",
        "@types/react": "^18.2.44",
        "commitlint": "^17.0.2",
        "del-cli": "^5.1.0",
        "eslint": "^8.51.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "jest": "^29.7.0",
        "prettier": "^3.0.3",
        "react": "18.3.1",
        "react-native": "0.76.1",
        "react-native-builder-bob": "^0.30.3",
        "release-it": "^15.0.0",
        "turbo": "^1.10.7",
        "typescript": "~5.1.6"
    },
    "resolutions": {
        "@types/react": "^18.2.44"
    },
    "peerDependencies": {
        "react": "*",
        "react-native": "*"
    },
    "packageManager": "yarn@3.6.1",
    "jest": {
        "preset": "react-native",
        "modulePathIgnorePatterns": [
            "<rootDir>/example/node_modules",
            "<rootDir>/lib/"
        ]
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "release-it": {
        "git": {
            "commitMessage": "chore: release ${version}",
            "tagName": "v${version}"
        },
        "npm": {
            "publish": true
        },
        "github": {
            "release": true
        },
        "plugins": {
            "@release-it/conventional-changelog": {
                "preset": "angular"
            }
        }
    },
    "eslintConfig": {
        "root": true,
        "extends": [
            "@react-native",
            "prettier"
        ],
        "rules": {
            "react/react-in-jsx-scope": "off",
            "prettier/prettier": [
                "error",
                {
                    "bracketSameLine": true,
                    "bracketSpacing": true,
                    "singleQuote": false,
                    "trailingComma": "none",
                    "semi": true,
                    "tabWidth": 4,
                    "printWidth": 120
                }
            ]
        }
    },
    "eslintIgnore": [
        "node_modules/",
        "lib/"
    ],
    "prettier": {
        "bracketSameLine": true,
        "bracketSpacing": true,
        "singleQuote": false,
        "trailingComma": "none",
        "semi": true,
        "tabWidth": 4,
        "printWidth": 120
    },
    "react-native-builder-bob": {
        "source": "src",
        "output": "lib",
        "targets": [
            [
                "commonjs",
                {
                    "esm": true
                }
            ],
            [
                "module",
                {
                    "esm": true
                }
            ],
            [
                "typescript",
                {
                    "project": "tsconfig.build.json",
                    "esm": true
                }
            ]
        ]
    },
    "create-react-native-library": {
        "type": "module-legacy",
        "languages": "kotlin-swift",
        "version": "0.42.2"
    },
    "dependencies": {
        "@callstack/react-theme-provider": "^3.0.9",
        "fuse.js": "^7.0.0",
        "google-libphonenumber": "^3.2.38",
        "node-emoji": "^2.1.3",
        "react-async-hook": "^4.0.0"
    },
    "directories": {
        "example": "example"
    }
}
